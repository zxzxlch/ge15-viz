<!doctype html>
<html class="no-js" lang="">
  <head>
    <meta charset="utf-8">
    <title>{{ title }}{{#if siteName}} - {{ siteName }}{{/if}}</title>
    <meta name="description" content="{{ description }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" href="{{ relativePath }}favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="{{ relativePath }}apple-touch-icon.png" />
    <link rel="apple-touch-icon" sizes="57x57" href="{{ relativePath }}apple-touch-icon-57x57.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="{{ relativePath }}apple-touch-icon-72x72.png" />
    <link rel="apple-touch-icon" sizes="76x76" href="{{ relativePath }}apple-touch-icon-76x76.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="{{ relativePath }}apple-touch-icon-114x114.png" />
    <link rel="apple-touch-icon" sizes="120x120" href="{{ relativePath }}apple-touch-icon-120x120.png" />
    <link rel="apple-touch-icon" sizes="144x144" href="{{ relativePath }}apple-touch-icon-144x144.png" />
    <link rel="apple-touch-icon" sizes="152x152" href="{{ relativePath }}apple-touch-icon-152x152.png" />
    <link rel="apple-touch-icon" sizes="180x180" href="{{ relativePath }}apple-touch-icon-180x180.png" />

    <!-- build:css(.) {{ relativePath }}styles/vendor.css -->
    <!-- bower:css -->
    <!-- endbower -->
    <!-- endbuild -->
    <!-- build:css(.tmp) {{ relativePath }}styles/main.css -->
    <link rel="stylesheet" href="{{ relativePath }}styles/main.css">
    <!-- endbuild -->
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400' rel='stylesheet' type='text/css'>
    <!-- build:js {{ relativePath }}scripts/vendor/modernizr.js -->
    <script src="{{ relativePath }}bower_components/modernizr/modernizr.js"></script>
    <!-- endbuild -->

    <!-- Facebook open graph tags -->
    <meta property="og:site_name" content="{{ siteName }}"/>
    <meta property="og:title" content="{{ title }}" />
    <meta property="og:description" content="{{ description }}" />
    {{#if ogImage}}<meta property="og:image" content="{{ relativePath }}{{ ogImage }}" />{{/if}}
    {{#if ogUrl}}<meta property="og:url" content="{{ ogUrl }}" />{{/if}}
    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="{{ siteName }}">
    <meta name="twitter:creator" content="@zxzxlch">
    <meta name="twitter:title" content="{{ title }}">
    <meta name="twitter:description" content="{{ description }}">
    {{#if ogImage}}<meta name="twitter:image" content="{{ relativePath }}{{ ogImage }}">{{/if}}

  </head>
  <body>
    <!--[if lt IE 10]>
      <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    <div class="container">

      {{> body }}

      <!-- Footer -->
      <div class="footer">
        <p>
          Made with <span class="glyphicon glyphicon-heart"></span> by <a href="https://twitter.com/zxzxlch">@zxzxlch</a>.
        </p>
      </div>
    </div>

    <!-- build:js(.) {{ relativePath }}scripts/vendor.js -->
    <!-- bower:js -->
    <script src="{{ relativePath }}bower_components/modernizr/modernizr.js"></script>
    <script src="{{ relativePath }}bower_components/jquery/dist/jquery.js"></script>
    <!-- endbower -->
    <!-- endbuild -->

    
    <!-- build:js(.) {{ relativePath }}scripts/plugins.js -->
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/affix.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/alert.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/dropdown.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/modal.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/transition.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/button.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/popover.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/carousel.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/scrollspy.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/collapse.js"></script>
    <script src="{{ relativePath }}bower_components/bootstrap-sass/assets/javascripts/bootstrap/tab.js"></script>
    <!-- endbuild -->

    {{#if includeScripts~}}
      <!-- Included scripts -->
      {{{includeScripts}}}
    {{~/if}}
    
    {{#with buildScripts~}}
      <!-- build:js(.tmp) {{ ../relativePath }}scripts/{{ main }}.js -->
      {{#each scripts~}}
        <script src="{{ ../../relativePath }}scripts/{{ this }}.js"></script>
      {{/each~}}
      <!-- endbuild -->
    {{~/with}}

    <!-- Google Analytics -->
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-31321179-3', 'auto');
      ga('send', 'pageview');

    </script>
  </body>
</html>
